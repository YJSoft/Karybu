<query id="isMemberSnsExisted" action="select">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="sns_guid" var="sns_guid" notnull="notnull" />
    </conditions>
</query>
