<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="en">Bootstrap Theme</title>
    <description xml:lang="en">Bootstrap theme of the Integrated Search Module</description>
     <version>0.1</version>
    <date>2007-07-24</date>

    <author email_address="dev@karybu.org" link="http://www.karybu.org/">
        <name xml:lang="ko">Arnia</name>
        <name xml:lang="vi">Arnia</name>
        <name xml:lang="zh-CN">Arnia</name>
        <name xml:lang="jp">Arnia</name>
        <name xml:lang="en">Arnia</name>
        <name xml:lang="zh-TW">Arnia</name>
